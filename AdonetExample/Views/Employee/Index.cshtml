@model List<AdonetExample.Models.EmployeeModel>
@{
    ViewBag.Title = "Index";
}

@Html.ActionLink("Create","Create")
<h2>Index</h2>

<table class="table table-bordered">
    <tr>
        <th>EmpId</th>
        <th>EmpName</th>
        <th>EmpSalary</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpId</td>
            <td>@item.EmpName</td>
            <td>@item.EmpSalary</td>
            <td>@Html.ActionLink("Edit","Edit",new {id=item.EmpId })</td>
            <td>@Html.ActionLink("Delete", "Delete", new { id = item.EmpId })</td>
        </tr>
    }
</table>